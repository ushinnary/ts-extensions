{
	"name": "ts-extensials",
	"version": "0.4.1",
	"description": "Add Rust and LINQ methods to TS base classes",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./array/rust": {
			"types": "./dist/array/array.rust.d.ts",
			"default": "./dist/array/array.rust.js"
		},
		"./array/linq": {
			"types": "./dist/array/array.linq.d.ts",
			"default": "./dist/array/array.linq.js"
		},
		"./number/custom": {
			"types": "./dist/number/number.custom.d.ts",
			"default": "./dist/number/number.custom.js"
		},
		"./number/rust": {
			"types": "./dist/number/number.rust.d.ts",
			"default": "./dist/number/number.rust.js"
		},
		"./string/rust": {
			"types": "./dist/string/string.rust.d.ts",
			"default": "./dist/string/string.rust.js"
		},
		"./string/custom": {
			"types": "./dist/string/string.custom.d.ts",
			"default": "./dist/string/string.custom.js"
		},
		"./helpers/StringBuilder": {
			"types": "./dist/helpers/StringBuilder.d.ts",
			"default": "./dist/helpers/StringBuilder.js"
		},
		"./helpers/Dictionary": {
			"types": "./dist/helpers/Dictionary.d.ts",
			"default": "./dist/helpers/Dictionary.js"
		}
	},
	"sideEffects": [
		"./dist/array/*.js",
		"./dist/number/*.js",
		"./dist/string/*.js",
		"./dist/index.js"
	],
	"type": "module",
	"files": [
		"dist/*"
	],
	"scripts": {
		"pretest": "nix-shell shell.nix --run \"npm run build\"",
		"test": "jest",
		"format": "biome format --write ./src",
		"format:tests": "biome format --write ./tests",
		"build": "tsc",
		"generate:doc": "node readme_fill.js"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ushinnary/ts-extensions"
	},
	"keywords": [
		"typescript"
	],
	"author": "ushinnary",
	"license": "MIT",
	"devDependencies": {
		"@babel/core": "^7.29.0",
		"@babel/preset-env": "^7.29.0",
		"@babel/preset-typescript": "^7.28.5",
		"@biomejs/biome": "2.3.15",
		"babel-jest": "^30.2.0",
		"jest": "^30.2.0",
		"typescript": "^5.9.3"
	}
}
